// FR2.3: Destructuring and spread dependencies

const defaultOptions: Options = {
    retries: 3,
    timeout: 5000
};
const [first, second] = values;
const { user: { profile: { name } } } = nested;
// Nested destructuring
const nested = {
    user: {
        profile: {
            name: 'test'
        }
    }
};
// Array destructuring
const values = [
    1,
    2,
    3
];
export function createClient({ retries, timeout }: Options = defaultOptions) {
    return {
        retries,
        timeout
    };
}
export const sum = first + second;
export const username = name;
// Function parameter destructuring
interface Options {
    timeout: number;
    retries: number;
}
const extendedConfig = {
    ...baseConfig,
    ssl: true
};
export const { host, port } = extendedConfig;
// Object with dependencies
const baseConfig = {
    host: 'localhost',
    port: 3000
};
