// FR3.3: Class members should be sorted within their groups
class MyClass {
    static bar = 'bar';
    static foo = 'foo';
    // Static fields out of order
    static zoo = 'zoo';
    apple: number;
    banana: boolean;
    // Instance fields out of order
    zebra: string;
    // Constructor should remain in place
    constructor(){
        this.apple = 1;
        this.banana = true;
        this.zebra = 'z';
    }
    static aStatic() {
        return 'a';
    }
    static bStatic() {
        return 'b';
    }
    // Static methods out of order
    static zStatic() {
        return 'z';
    }
    aMethod() {
        return this.apple;
    }
    bMethod() {
        return this.banana;
    }
    // Instance methods out of order
    zMethod() {
        return this.zebra;
    }
}
