// FR3.3: Class members should be sorted within visibility groups

class MyClass {
    static bar = 'bar';
    static foo = 'foo';
    static zoo = 'zoo';

    static #privateBar = 'pbar';
    static #privateFoo = 'pfoo';
    static #privateZoo = 'pzoo';

    static aStatic() {
        return 'a';
    }
    static bStatic() {
        return 'b';
    }
    static zStatic() {
        return 'z';
    }

    static #privateAStatic() {
        return 'pa';
    }
    static #privateBStatic() {
        return 'pb';
    }
    static #privateZStatic() {
        return 'pz';
    }

    apple: number;
    banana: boolean;
    zebra: string;

    #privateApple = 1;
    #privateBanana = true;
    #privateZebra = 'pz';

    constructor(){
        this.apple = 1;
        this.banana = true;
        this.zebra = 'z';
    }

    aMethod() {
        return this.apple;
    }
    bMethod() {
        return this.banana;
    }
    zMethod() {
        return this.zebra;
    }

    #privateAMethod() {
        return this.#privateApple;
    }
    #privateBMethod() {
        return this.#privateBanana;
    }
    #privateZMethod() {
        return this.#privateZebra;
    }
}
